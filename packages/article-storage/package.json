    "cross-env": "^7.0.3",
    "eslint": "^8.17.0",
    "eslint-config-alloy": "^4.5.1",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.75.5",
    "rollup-plugin-filesize": "^9.1.2",
